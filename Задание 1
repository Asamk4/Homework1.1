#Создать лист из 6 любых чисел. Отсортировать его по возрастанию
list = [3, 1, 55, 2, 9, 44]
y = -1
x = -1
z = len(list)
for num_1 in list:
    if x >= z:
        break
    y += 1
    for num_2 in list:
        x += 1
        if x >= z:
            break
        if list[y] > list[x]:
            list[y] = list[x]
            list[x] = num_1
            x -= 1
            break
